# Podman Network definition for pf-web services
[Unit]
Description=pf-web Internal Network
Documentation=https://github.com/P4X-ng/pf-web-poly-compile-helper-runner

[Network]
NetworkName=pf-web
Driver=bridge
Subnet=10.89.0.0/24
Gateway=10.89.0.1
IPv6=false

# Enable inter-container communication
Internal=false

[Install]
WantedBy=default.target
